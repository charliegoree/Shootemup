{
  "name": "Shootall",
  "version": "0.0.1",
  "description": "Juego de Navecitas wachin",
  "main": "main.js",
  "scripts": {
    "build": "webpack --progress --profile --colors",
    "package": "electron-packager ./ --platform=linux --arch=ia32 --out=package --asar=true --overwrite=force",
    "package-win": "electron-packager ./ --platform=win32 --arch=ia32 --icon=assets/app.ico --out=package --asar=true --overwrite=force",
    "start": "electron .",
    "dev": "npm run build && npm start"
  },
  "author": "Charliegore",
  "license": "MIT",
  "devDependencies": {
    "electron": "^1.4.1",
    "electron-packager": "^8.5.0",
    "webpack": "2.1.0-beta.22",
    "electron-reload": "^1.0.2",
    "babel-core": "^6.16.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.16.0",
    "cross-env": "^3.1.1",
    "css-loader": "^0.25.0",
    "file-loader": "^0.9.0",
    "vue-loader": "^9.5.1",
    "webpack-dev-server": "^2.1.0-beta.0"
  },
  "dependencies": {
    "electron": "^1.4.3",
    "firebase": "^3.6.5",
    "lowdb": "^0.14.0"
  }
}
